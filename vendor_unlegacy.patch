+# Unless required by applicable law or agreed to in writing, software
+# distributed under the License is distributed on an "AS IS" BASIS,
+# WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
+# See the License for the specific language governing permissions and
+# limitations under the License.
+
+# Inherit Unlegacy Android common configuration
+$(call inherit-product, vendor/unlegacy/configs/common.mk)
+
+# Inherit AOSP device configuration
+$(call inherit-product, device/samsung/galaxysubi/aosp_galaxysubi.mk)
+
+PRODUCT_NAME   := ua_galaxysubi
+PRODUCT_DEVICE := galaxysubi
+PRODUCT_BRAND  := Samsung
+PRODUCT_MODEL  := GT-I9000
+PRODUCT_MANUFACTURER := Samsung
+
+# Device build info and fingerprint
+PRODUCT_BUILD_PROP_OVERRIDES += \
+    PRODUCT_NAME=GT-I9000 \
+    TARGET_DEVICE=GT-I9000 \
+    BUILD_FINGERPRINT="samsung/GT-I9000/GT-I9000:2.3.5/GINGERBREAD/XXJVT:user/release-keys" \
+    PRIVATE_BUILD_DESC="GT-I9000-user 2.3.5 GINGERBREAD XXJVT release-keys"
diff --git a/vendorsetup.sh b/vendorsetup.sh
index 01b38a6..619013b 100644
--- a/vendorsetup.sh
+++ b/vendorsetup.sh
@@ -12,6 +12,7 @@ add_lunch_combo ua_espresso3g-userdebug
 add_lunch_combo ua_flo-userdebug
 add_lunch_combo ua_flounder-userdebug
 add_lunch_combo ua_flounder_lte-userdebug
+add_lunch_combo ua_galaxysubi-userdebug
 add_lunch_combo ua_grouper-userdebug
 add_lunch_combo ua_hummingbird-userdebug
 add_lunch_combo ua_hammerhead-userdebug
